{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "React-TypeScript 工程化开发",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint src --ext .tsx",
    "lint:fix": "eslint src --ext .tsx --fix",
    "test": "jest",
    "changelogs": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wuxuweilalala/react-ts-template.git"
  },
  "keywords": [
    "typescript",
    "react"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "author": "wuxuwei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wuxuweilalala/react-ts-template/issues"
  },
  "homepage": "https://github.com/wuxuweilalala/react-ts-template#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.4.1",
    "hoist-non-react-statics": "^3.3.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "source-map-loader": "^3.0.0",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "connected-react-router": "^6.9.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.1.1"
  }
}
